{
  "name": "TokFox",
  "description": "TokFox demo",
  "launch_path": "/demo.html",
  "orientation": "portrait",
  "type": "privileged",
  "default_locale": "en",
  "icons": {
    "60": "/icon.png"
  },
  "permissions": {
    "desktop-notification": {},
    "push": {},
    "systemXHR": {},
    "video-capture": {},
    "audio-capture": {}
  },
  "messages": [
    { "push": "/index.html"},
    { "push-register": "/index.html"},
    { "notification": "/index.html" }
  ]
}
